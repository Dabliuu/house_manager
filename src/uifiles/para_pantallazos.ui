QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	/*image: url(./icons/c_p.png);*/
   	background: qlineargradient(
	spread:pad,
	 x1:0, y1:0, x2:0, y2:1,
	stop:0 #136559, stop:1 #1D9886);
	
}



QComboBox::drop-down { /*the box were the down arrow resides*/
